#
# Generated by Chef for <%= node['fqdn'] %>
# Do NOT modify this file by hand, any changes will be overwritten.
#

# Source - <%= @description %>
<% if parameter_defined?(@source) -%>
source <%= @name -%> {
    <% @source.each do |config| -%>
    <% config.each do |driver, parameters| -%>
    <% result = source_builder(driver: driver, parameters: parameters, multiline: @multiline) -%>
    <% if result.is_a?(String) -%>
    <%= result %>
    <% elsif result.is_a?(Array) -%>
    <% result.each do |line| -%>
    <%= line %>
    <% end -%>
    <% end -%>
    <% end -%>
    <% end -%>
};
<% end -%>
